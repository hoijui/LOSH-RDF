    okh:spdxLicense <https://spdx.org/licenses/CC-BY-SA-4.0> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:53940> .

:Image a okh:Image ;
