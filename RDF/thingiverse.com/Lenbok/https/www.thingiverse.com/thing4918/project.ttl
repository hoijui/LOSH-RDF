@prefix okh: <https://github.com/OPEN-NEXT/OKH-LOSH/raw/master/OKH-LOSH.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:OutlinePluginForSkeinforge a okh:Module ;
    rdfs:label "outline plugin for skeinforge" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:function """When printing raftless it can be helpful to add a bounding box type extrusion on the first layer of the print, kind of like nophead uses (see http://hydraraptor.blogspot.com/2010/01/hot-metal-and-serendipity.html\r
). This outline gives the extruder some extra time to begin extruding properly before beginning your object, and so you get an early verification of where your object\r
will sit on the platform.  Being a plugin you can turn the outline on and off as part of your usual skeinforge profiles.""" ;
    okh:hasImage :Image ;
    okh:licensor "Lenbok" ;
    okh:repo <https://www.thingiverse.com/thing:4918> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/GPL-3.0-or-later> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:4918> .

:Image a okh:Image ;
    rdfs:label "Image of outline plugin for skeinforge 1.0.0" ;
    okh:fileFormat "JPG" ;
