@prefix : <https://www.thingiverse.com/thing%3A9292/1.0.0/> .
@prefix okh: <https://github.com/OPEN-NEXT/OKH-LOSH/raw/master/OKH-LOSH.ttl#> .
@prefix otrl: <http://purl.org/oseg/ontologies/OTRL#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Project a okh:Module ;
    rdfs:label "Thingiverse Downloader" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:documentationReadinessLevel otrl:Odrl3 ;
    okh:function """This is a simple (rather simple, was my first dip into Perl) script to download and organize a Thing, placing it's various parts into ./thing/\r
It also outputs a URL shortcut to the page on thingiverse.""" ;
    okh:hasImage :Image ;
    okh:licensor "feilen" ;
    okh:repo <https://www.thingiverse.com/thing:9292> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/CC-BY-SA-4.0> ;
    okh:technologyReadinessLevel otrl:Otrl4 ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:9292> .

:Image a okh:Image ;
    rdfs:label "Image of Thingiverse Downloader 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/renders/97/26/63/e3/04/Screenshot_display_large_display_large.jpg> ;
    okh:permaURL <https://cdn.thingiverse.com/renders/97/26/63/e3/04/Screenshot_display_large_display_large.jpg> .

