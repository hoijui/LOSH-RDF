@prefix okh: <https://github.com/OPEN-NEXT/OKH-LOSH/raw/master/OKH-LOSH.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:ExportOpenscadPathsFromAdobeIllustrator a okh:Module ;
    rdfs:label "Export OpenSCAD paths from Adobe Illustrator" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:function """This is a plugin for Adobe Illustrator (CS3, Windows) to export paths (and only straight-line paths!) to an OpenSCAD file.\r
\r
Since Illustrator CS3 (and higher) exports only DXF R13+, it'll generate POLYLINE and other objects, which OpenSCAD does not support.  By using this export plugin, you'll be able to use the path directly as a polygon().\r
\r
(Supposedly CS3 plugins are loadable with CS4 and CS5.)""" ;
    okh:hasImage :Image ;
    okh:licensor "kitlaan" ;
    okh:repo <https://www.thingiverse.com/thing:9529> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/CC-BY-SA-4.0> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:9529> .

:Image a okh:Image ;
    rdfs:label "Image of Export OpenSCAD paths from Adobe Illustrator 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/site/img/default/Gears_display_large.jpg> ;
    okh:permaURL <https://cdn.thingiverse.com/site/img/default/Gears_display_large.jpg> .

