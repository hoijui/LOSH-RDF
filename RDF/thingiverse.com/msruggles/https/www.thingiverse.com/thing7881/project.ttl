@prefix okh: <https://github.com/OPEN-NEXT/OKH-LOSH/raw/master/OKH-LOSH.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:GearProfileGenerator a okh:Module ;
    rdfs:label "Gear Profile Generator" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:function """This is a spreadsheet that will generate a list of points for half a gear tooth (image shows CAD result for 3 teeth) depending on the number of teeth, pressure angle and profile offset. From there you can load it into your CAD software of choice. \r
\r
I've based this off of the equations and methodology from the following:\r
http://school.mech.uwa.edu.au/~dwright/DANotes/gears/appendices.pdf (Appendix B)\r
and:\r
http://school.mech.uwa.edu.au/~dwright/DANotes/gears/generation/generation.html#top\r
\r
I've given up on trying to code this in any way.""" ;
    okh:hasImage :Image ;
    okh:licensor "msruggles" ;
    okh:repo <https://www.thingiverse.com/thing:7881> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/GPL-3.0-or-later> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:7881> .

:Image a okh:Image ;
    rdfs:label "Image of Gear Profile Generator 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/renders/8c/f6/12/0b/ab/Untitled_display_large_display_large.jpg> ;
    okh:permaURL <https://cdn.thingiverse.com/renders/8c/f6/12/0b/ab/Untitled_display_large_display_large.jpg> .

