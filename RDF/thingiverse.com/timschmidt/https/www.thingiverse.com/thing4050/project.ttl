:ModifiedFrameVertex a okh:Module ;
    rdfs:label "Modified Frame Vertex" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:function "Two threaded-rod cross-bars through the top-most frame-vertices aren't really necessary for mendel's structural integrity.  Further, the z-bar-top-clamp parts are larger than necessary.  This is a modified frame vertex that's smaller than the standard one, requires only one threaded-rod cross-bar, and uses whowhatsis's pinch block y bar clamp ( http://www.thingiverse.com/thing:3843 ) instead of the z-bar-top-clamp for additional printed part savings." ;
    okh:hasImage :Image ;
    okh:licensor "timschmidt" ;
    okh:repo <https://www.thingiverse.com/thing:4050> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/GPL-3.0-or-later> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:4050> .

:Image a okh:Image ;
    rdfs:label "Image of Modified Frame Vertex 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/site/img/default/Gears_display_large.jpg> ;
