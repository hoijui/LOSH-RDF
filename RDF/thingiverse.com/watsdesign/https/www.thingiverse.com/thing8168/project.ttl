@prefix okh: <https://github.com/OPEN-NEXT/OKH-LOSH/raw/master/OKH-LOSH.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:LeatherGloveScan a okh:Module ;
    rdfs:label "Leather glove scan" ;
    okh:dataSource "thingiverse.com" ;
    okh:documentationLanguage "en" ;
    okh:function """After seeing how http://www.my3dscanner.com works I wanted to give it a try.\r
\r
A leather glove on a quite dirty cutting mat were good textured things (enjoy these rules of imperfection), and hard to model otherwise.\r
I was surprised by the result of this first try and the simplicity of the process. With just a backlight to avoid the shadow it would be even better !\r
\r
(I'll maybe upload an stl if I can pursue the reconstruction, or someone else ^^)\r
\r
[edit:]\r
See : http://www.thingiverse.com/thing:8229\r
And http://www.thingiverse.com/thing:8232 for the printable version !""" ;
    okh:hasImage :Image ;
    okh:licensor "watsdesign" ;
    okh:repo <https://www.thingiverse.com/thing:8168> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/CC-BY-SA-4.0> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:8168> .

:Image a okh:Image ;
    rdfs:label "Image of Leather glove scan 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/renders/64/cd/ee/2a/07/snapshot00_display_large_display_large.jpg> ;
    okh:permaURL <https://cdn.thingiverse.com/renders/64/cd/ee/2a/07/snapshot00_display_large_display_large.jpg> .

