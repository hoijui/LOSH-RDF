    okh:hasImage :Image ;
    okh:licensor "wurp" ;
    okh:repo <https://www.thingiverse.com/thing:6722> ;
    okh:repoHost "www.thingiverse.com" ;
    okh:spdxLicense <https://spdx.org/licenses/LGPL-3.0-or-later> ;
    okh:uploadMethod "auto" ;
    okh:version "1.0.0" ;
    okh:versionOf <https://www.thingiverse.com/thing:6722> .

:Image a okh:Image ;
    rdfs:label "Image of Simple model slicer 1.0.0" ;
    okh:fileFormat "JPG" ;
    okh:fileUrl <https://cdn.thingiverse.com/site/img/default/Gears_display_large.jpg> ;
